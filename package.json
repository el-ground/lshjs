{
  "name": "lshjs",
  "version": "1.0.8",
  "description": "Module which allows query strings to be placed into LSH batches, and compares candidates from a list of LSH candidates",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "git@github.com:el-ground/lshjs.git",
  "author": "<sjeon@elground.com>",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "set-env": " echo set environment and temp files here!",
    "dev": "cross-env PORT=9023 HTTPS_PORT=9646 NODE_ENV=development CERT=$DEV_CERT CERT_KEY=$DEV_CERT_KEY nodemon dist/src/index.js",
    "package": "yarn workspaces focus --all && tsc --project tsconfig.src.json && tsc-alias -p tsconfig.src.json",
    "_package:listen": "yarn clean && yarn set-env && tsc-watch --project tsconfig.src.json --onSuccess \"tsc-alias -p tsconfig.src.json\"",
    "package:listen": "cross-env ENVIRONMENT=development yarn _package:listen",
    "typecheck": "tsc --project tsconfig.src.json",
    "lint:js": "eslint --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "prettier": "prettier --check --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "lint": "yarn lint:js & yarn prettier",
    "test": "jest --coverage --passWithNoTests --maxWorkers=1",
    "test:watch": "jest --watchAll --coverage --passWithNoTests --maxWorkers=1",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "gcp-build": "echo run gcp build script here!",
    "_deploy": "yarn clean && yarn set-env && yarn package && gcloud app deploy gae-deployment.yaml",
    "deploy:staging": "cross-env ENVIRONMENT=staging yarn _deploy",
    "deploy:production": "cross-env ENVIRONMENT=production yarn _deploy",
    "example": "yarn package && node dist/src/examples/index.js"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-request-id": "^3.0.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.1.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.0",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.7.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.4.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "hangul-js": "^0.2.6",
    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.10",
    "winston": "^3.8.2"
  },
  "packageManager": "yarn@3.2.3"
}
